{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "outDir": "esm",
    "noImplicitAny": false,
    "target": "es5",
    "module": "esnext",
    "lib": ["es2019", "dom"],
    "allowJs": true,
    "sourceMap": true,
    "declaration": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts", "test/**/*.ts"],
  "exclude": ["src/umd.ts"]
}
